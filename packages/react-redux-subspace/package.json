{
  "name": "react-redux-subspace",
  "version": "6.1.0",
  "description": "react-redux Provider extension for redux-subspace",
  "author": "Michael Peyper",
  "contributors": [
    "Jonathan Peyper",
    "Vivian Farrell"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "src/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf lib es",
    "build": "rollup -c",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix",
    "test": "cross-env NODE_ENV=test nyc mocha --recursive --require @babel/register --require jsdom-global/register --require ./test/setup.js $(find test -name '*-spec.js')",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ioof-holdings/redux-subspace.git"
  },
  "dependencies": {
    "@types/react": "^16.8.23",
    "@types/react-redux": "^7.1.1",
    "hoist-non-react-statics": "^3.3.0",
    "prop-types": "^15.7.2",
    "recompose": "^0.30.0",
    "redux-subspace": "^6.1.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-redux": "^7.1.1",
    "redux": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-modules-commonjs": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "@babel/register": "7.7.0",
    "@testing-library/react-hooks": "3.2.1",
    "babel-eslint": "10.0.3",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "chai": "4.2.0",
    "cross-env": "6.0.3",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.6.0",
    "eslint-plugin-react": "7.16.0",
    "jsdom": "15.2.1",
    "jsdom-global": "3.0.2",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "7.1.3",
    "react-test-renderer": "16.11.0",
    "redux": "4.0.4",
    "redux-mock-store": "1.5.3",
    "rimraf": "3.0.0",
    "rollup": "1.26.3",
    "rollup-plugin-babel": "4.3.3",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0",
    "typescript": "3.6.4",
    "typescript-definition-tester": "0.0.6"
  },
  "nyc": {
    "temp-directory": "coverage/.nyc_output",
    "reporter": [
      "html",
      "text",
      "json"
    ]
  }
}
